		if(arrRows.length >=1){
			this.arr.push(this.sun);
		}
	}
}

this.Button00_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var sReturn="";
	this.fn_addArray("mon");
	this.fn_addArray("tue");
	this.fn_addArray("wen");
	this.fn_addArray("tur");
	this.fn_addArray("fri");
	this.fn_addArray("sat");
	this.fn_addArray("sun");
	for(var i =0; i<this.arr.length; i++){
		sReturn += this.arr[i] +")|";
	}
	this.close(sReturn);
};
