{
	
};
this.fn_addArray = function(id){
	var arrRows;
	if(id=="mon"){
		arrRows = this.ds_scheduler.extractRows( ""+id+" == 1" );
		for(var i=0; i<arrRows.length; i++){
 			this.mon.push(arrRows[i]+1);
 		}
		if(arrRows.length >=1){
			this.arr.push(this.mon);
		}
	}else if(id=="tue"){
		arrRows = this.ds_scheduler.extractRows( ""+id+" == 1" );
		for(var i=0; i<arrRows.length; i++){
 			this.tue.push(arrRows[i]+1);
 		}
		if(arrRows.length >=1){
			this.arr.push(this.tue);
		}
	}else if(id=="wen"){
		arrRows = this.ds_scheduler.extractRows( ""+id+" == 1" );
		for(var i=0; i<arrRows.length; i++){
 			this.wen.push(arrRows[i]+1);
 		}
		if(arrRows.length >=1){
			this.arr.push(this.wen);
		}
	}else if(id=="tur"){
		arrRows = this.ds_scheduler.extractRows( ""+id+" == 1" );
		for(var i=0; i<arrRows.length; i++){
 			this.tur.push(arrRows[i]+1);
 		}
		if(arrRows.length >=1){
			this.arr.push(this.tur);
		}
	}else if(id=="fri"){
		arrRows = this.ds_scheduler.extractRows( ""+id+" == 1" );
		for(var i=0; i<arrRows.length; i++){
 			this.fri.push(arrRows[i]+1);
 		}
		if(arrRows.length >=1){
			this.arr.push(this.fri);
		}
	}else if(id=="sat"){
		arrRows = this.ds_scheduler.extractRows( ""+id+" == 1" );
		for(var i=0; i<arrRows.length; i++){
 			this.sat.push(arrRows[i]+1);
 		}
		if(arrRows.length >=1){
			this.arr.push(this.sat);
		}
	}else if(id=="sun"){
		arrRows = this.ds_scheduler.extractRows( ""+id+" == 1" );
		for(var i=0; i<arrRows.length; i++){
 			this.sun.push(arrRows[i]+1);
